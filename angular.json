{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "b0f6e55c-ab84-4499-9f50-0848270d0f5d"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "infratec": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "infratec",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../build/infratec",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/_redirects"
            ],
            "allowedCommonJsDependencies": [
              "sweetalert2/dist/sweetalert2.js",
              "chartjs-plugin-datalabels",
              "@babel/runtime-corejs3",
              "crypto-js/hmac-sha256",
              "crypto-js/enc-base64",
              "@ctrl/ngx-codemirror",
              "crypto-js/enc-utf8",
              "devexpress-diagram",
              "devextreme-angular",
              "devextreme-quill",
              "devexpress-gantt",
              "lodash.debounce",
              "lodash.throttle",
              "jspdf-autotable",
              "lodash.memoize",
              "chartjs-gauge",
              "can-use-dom",
              "file-saver",
              "devextreme",
              "chart.js",
              "rgbcolor",
              "exceljs",
              "core-js",
              "moment",
              "jszip",
              "lodash",
              "uuid",
              "raf",
              "flat"
            ],
            "styles": [
              "node_modules/devextreme/dist/css/dx.common.css",
              "node_modules/devexpress-diagram/dist/dx-diagram.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "node_modules/sweetalert2/src/sweetalert2.scss",
              "src/@dev-express/generated/theme.additional.css",
              "src/@dev-express/generated/theme.base.css",
              "src/@dev-express/dx-styles.scss",
              "src/@sweet-alert2/sweet-alert-styles.scss",
              "src/@fuse/styles/tailwind.scss",
              "src/@fuse/styles/themes.scss",
              "src/styles/vendors.scss",
              "src/@fuse/styles/main.scss",
              "src/styles/tailwind.scss",
              "src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/@fuse/styles"
              ]
            },
            "scripts": [],
            "baseHref": "/",
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": false
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "analyze": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "statsJson": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "infratec:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "infratec:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "infratec:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/manifest.json"
            ]
          }
        }
      }
    },
    "infratec-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./e2e/protractor.conf.js",
            "devServerTarget": "infratec:serve"
          }
        }
      }
    }
  }
}
